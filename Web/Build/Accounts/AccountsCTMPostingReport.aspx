<%@ Page Language="C#" AutoEventWireup="true" CodeFile="AccountsCTMPostingReport.aspx.cs" Inherits="AccountsCTMPostingReport" %>

<%@ Register TagPrefix="eluc" TagName="TabStrip" Src="~/UserControls/UserControlTabsTelerik.ascx" %>
