<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head runat="server">
    <title>Invoice Clarification</title>
    <telerik:RadCodeBlock ID="RadCodeBlock1" runat="server">
        <%: Scripts.Render("~/bundles/js") %>
        <%: Styles.Render("~/bundles/css") %>
    </telerik:RadCodeBlock>
</head>
<body>
    <form id="frmDebitCreditNote" runat="server" submitdisabledcontrols="true">
        <telerik:RadScriptManager runat="server" ID="RadScriptManager1" />
        <telerik:RadAjaxPanel ID="RadAjaxPanel1" runat="server" LoadingPanelID="RadAjaxLoadingPanel1" Height="100%">
            <eluc:Error ID="ucError" runat="server" Text="" Visible="false"></eluc:Error>
            <eluc:Status runat="server" ID="ucStatus" />
            <%--  <eluc:Title runat="server" ID="ucTitle" Text="Invoice Query"></eluc:Title>--%>
            <asp:Button ID="cmdHiddenSubmit" runat="server" Text="cmdHiddenSubmit" OnClick="cmdHiddenSubmit_Click"
                CssClass="hidden" />
            <eluc:TabStrip ID="MenuInvoiceClarification" runat="server" OnTabStripCommand="MenuInvoiceClarification_TabStripCommand"
                TabStrip="true"></eluc:TabStrip>

            <table cellpadding="2" cellspacing="2" style="width: 100%">
                <tr>
                    <td>
                        <telerik:RadLabel ID="lblClarification" runat="server" Text="Clarification :"></telerik:RadLabel>
                        <br />
                        <telerik:RadTextBox ID="txtClarification" runat="server" CssClass="input" TextMode="MultiLine" Width="90%" Height="50px"></telerik:RadTextBox>
                    </td>
                </tr>
            </table>
        </telerik:RadAjaxPanel>
    </form>
</body>
</html>
